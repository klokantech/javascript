{
  "id": "kt",

  "inputs": ["src/main.js"],
  "paths": ["src"],

  "externs": [
    "./externs/ol3-3.16.0.js",
    "./externs/ol3-extra.js"
  ],

  "define": {
    "goog.DEBUG": false,
    "kt.expose.GENERATE": true
  },

  "mode": "ADVANCED",
  "level": "VERBOSE",

  "checks": {
    "accessControls": "ERROR",
    "visibility": "ERROR",
    "checkTypes": "ERROR",
    "checkRegExp": "ERROR",
    "checkVars": "ERROR",
    "deprecated": "ERROR",
    "fileoverviewTags": "ERROR",
    "invalidCasts": "ERROR",
    "missingProperties": "ERROR",
    "nonStandardJsDocs": "ERROR",
    "undefinedVars": "ERROR"
  },

  "jsdoc-html-output-path": "."
}
